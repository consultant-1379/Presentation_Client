<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:EnumDataTypeDefinition xmlns:ns1="urn:com:ericsson:schema:xml:oss:oss_common" xmlns:ns2="urn:com:ericsson:schema:xml:oss:oss_edt" xmlns:ns3="urn:com:ericsson:schema:xml:oss:dps_primarytype" xmlns:ns4="urn:com:ericsson:schema:xml:oss:cbp_engine" xmlns:ns5="urn:com:ericsson:schema:xml:oss:cbp_common" xmlns:ns6="urn:com:ericsson:schema:xml:oss:cbp_component" xmlns:ns7="urn:com:ericsson:schema:xml:oss:dps_relationship" xmlns:ns8="urn:com:ericsson:schema:xml:oss:oss_cache" xmlns:ns9="urn:com:ericsson:schema:xml:oss:oss_channel" xmlns:ns10="urn:com:ericsson:schema:xml:oss:oss_confparam" xmlns:ns11="urn:com:ericsson:schema:xml:oss:cbp_flow" xmlns:ns12="urn:com:ericsson:schema:xml:oss:oss_event" xmlns:ns13="urn:com:ericsson:schema:xml:oss:oss_annotation" xmlns:ns14="urn:com:ericsson:schema:xml:oss:oss_cdt" ns1:ns="ERBS_NODE_MODEL_D" ns1:name="CertEnrollState" ns1:version="0.0.1" ns1:lifeCycle="NORMAL">
    <ns1:modelCreationInfo creationDate="2013-06-26 11:06:21.849" author="mom-xml-transformer-tool">
        <ns1:derivedModel urn="/oss_edt/ERBS_NODE_MODEL_D/ERBS_NODE_MODEL_D/0.0.1"/>
    </ns1:modelCreationInfo>
    <ns1:desc>Certificate Enrollment State</ns1:desc>
    <ns2:member value="0" ns1:name="IDLE" ns1:lifeCycle="NORMAL">
        <ns1:desc>Idle. An initCertEnrollment request will be accepted.</ns1:desc>
    </ns2:member>
    <ns2:member value="1" ns1:name="PREPARING_REQUEST" ns1:lifeCycle="NORMAL">
        <ns1:desc>The node is preparing the certificate enrollment sequence.</ns1:desc>
    </ns2:member>
    <ns2:member value="2" ns1:name="POLLING" ns1:lifeCycle="NORMAL">
        <ns1:desc>An enrollment request has been sent to the enrollment server. The node is polling for a new certificate but the requested certificate has not yet been issued.</ns1:desc>
    </ns2:member>
    <ns2:member value="3" ns1:name="NEW_CREDS_AWAIT_CONF" ns1:lifeCycle="NORMAL">
        <ns1:desc>Awaiting confirmation for the use of new credentials. An event is issued.</ns1:desc>
    </ns2:member>
    <ns2:member value="4" ns1:name="ERROR" ns1:lifeCycle="NORMAL">
        <ns1:desc>An error has occurred. An alarm is issued.
				
				See the certEnrollErrorMsg attribute for a description of the error.
				
				The only way to leave this state is by using the action, cancelCertEnrollment.</ns1:desc>
    </ns2:member>
</ns2:EnumDataTypeDefinition>
