        <ns1:desc>This parameter defines the minimum target number of UEs in each cell, that are assigned ANR specific thresholds for EUTRAN intra-frequency neighbour cell reports. The actual number of UEs with these thresholds may deviate from the target.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longType">
            <ns1:valueRangeConstraint>
                <ns1:minMaxRange min="0" max="100"/>
            </ns1:valueRangeConstraint>
        </ns1:type>
        <ns1:default xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longValue" value="1"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="false" ns1:name="anrUesEUtraIntraFMax" ns1:lifeCycle="NORMAL">
        <ns1:desc>This parameter defines the maximum target number of UEs in each cell, that are assigned ANR specific thresholds for EUTRAN intra-frequency neighbour cell reports. The actual number of UEs with these thresholds may deviate from the target.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longType">
            <ns1:valueRangeConstraint>
                <ns1:minMaxRange min="0" max="100"/>
            </ns1:valueRangeConstraint>
        </ns1:type>
        <ns1:default xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longValue" value="20"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="false" ns1:name="hoAllowedEutranPolicy" ns1:lifeCycle="NORMAL">
        <ns1:desc>Starting value of the parameter isHoAllowed when a EUtranCellRelation is created by ANR (Automatic Neighbor Relations) or X2.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:booleanType"/>
        <ns1:default xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:booleanValue" value="true"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="false" ns1:name="anrUesEUtraIntraFIncrAnr" ns1:lifeCycle="NORMAL">
        <ns1:desc>This parameter defines the increase in the target number of UEs in each cell, that are assigned ANR specific thresholds for EUTRAN intra-frequency neighbour cell reports, every time a neighbour cell report for ANR is received with unknown neighbour cells.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longType">
            <ns1:valueRangeConstraint>
                <ns1:minMaxRange min="0" max="1000"/>
            </ns1:valueRangeConstraint>
        </ns1:type>
        <ns1:default xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longValue" value="20"/>
    </ns2:primaryTypeAttribute>
</ns2:PrimaryTypeDefinition>
