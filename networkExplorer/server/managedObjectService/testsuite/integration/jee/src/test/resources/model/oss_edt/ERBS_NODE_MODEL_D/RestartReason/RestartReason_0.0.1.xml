<ns2:EnumDataTypeDefinition xmlns:ns1="urn:com:ericsson:schema:xml:oss:oss_common" xmlns:ns2="urn:com:ericsson:schema:xml:oss:oss_edt" xmlns:ns3="urn:com:ericsson:schema:xml:oss:dps_primarytype" xmlns:ns4="urn:com:ericsson:schema:xml:oss:cbp_engine" xmlns:ns5="urn:com:ericsson:schema:xml:oss:cbp_common" xmlns:ns6="urn:com:ericsson:schema:xml:oss:cbp_component" xmlns:ns7="urn:com:ericsson:schema:xml:oss:dps_relationship" xmlns:ns8="urn:com:ericsson:schema:xml:oss:oss_cache" xmlns:ns9="urn:com:ericsson:schema:xml:oss:oss_channel" xmlns:ns10="urn:com:ericsson:schema:xml:oss:oss_confparam" xmlns:ns11="urn:com:ericsson:schema:xml:oss:cbp_flow" xmlns:ns12="urn:com:ericsson:schema:xml:oss:oss_event" xmlns:ns13="urn:com:ericsson:schema:xml:oss:oss_annotation" xmlns:ns14="urn:com:ericsson:schema:xml:oss:oss_cdt" ns1:ns="ERBS_NODE_MODEL_D" ns1:name="RestartReason" ns1:version="0.0.1" ns1:lifeCycle="NORMAL">
    <ns1:modelCreationInfo creationDate="2013-06-26 11:06:21.849" author="mom-xml-transformer-tool">
        <ns1:derivedModel urn="/oss_edt/ERBS_NODE_MODEL_D/ERBS_NODE_MODEL_D/0.0.1"/>
    </ns1:modelCreationInfo>
    <ns1:desc>The restart reason.</ns1:desc>
    <ns2:member value="0" ns1:name="PLANNED_RECONFIGURATION" ns1:lifeCycle="NORMAL">
        <ns1:desc>Restart ordered to perform planned activities such as reconfigurations.</ns1:desc>
    </ns2:member>
    <ns2:member value="1" ns1:name="UNPLANNED_NODE_EXTERNAL_PROBLEMS" ns1:lifeCycle="NORMAL">
        <ns1:desc>Restart ordered to resolve node external problems.</ns1:desc>
    </ns2:member>
    <ns2:member value="2" ns1:name="UNPLANNED_NODE_UPGRADE_PROBLEMS" ns1:lifeCycle="NORMAL">
        <ns1:desc>Restart ordered to resolve upgrade problems.</ns1:desc>
    </ns2:member>
    <ns2:member value="3" ns1:name="UNPLANNED_O_AND_M_ISSUE" ns1:lifeCycle="NORMAL">
        <ns1:desc>Restart ordered to resolve O&amp;M issues.</ns1:desc>
    </ns2:member>
    <ns2:member value="4" ns1:name="UNPLANNED_CYCLIC_RECOVERY" ns1:lifeCycle="NORMAL">
        <ns1:desc>Restart ordered to stop further auto recovery attempts.</ns1:desc>
    </ns2:member>
    <ns2:member value="5" ns1:name="UNPLANNED_LOCKED_RESOURCES" ns1:lifeCycle="NORMAL">
        <ns1:desc>Restart ordered to release locked resources.</ns1:desc>
    </ns2:member>
    <ns2:member value="6" ns1:name="UNPLANNED_COLD_WITH_HW_TEST" ns1:lifeCycle="NORMAL">
        <ns1:desc>Restart ordered to perform HW tests.</ns1:desc>
    </ns2:member>
    <ns2:member value="7" ns1:name="UNPLANNED_CALL_PROCESSING_DEGRADATION" ns1:lifeCycle="NORMAL">
        <ns1:desc>Restart ordered to improve Call Set Up or Call Drop Rate.</ns1:desc>
    </ns2:member>
    <ns2:member value="8" ns1:name="UNPLANNED_LOW_COVERAGE" ns1:lifeCycle="NORMAL">
        <ns1:desc>Restart ordered to improve Cell Availability.</ns1:desc>
    </ns2:member>
    <ns2:member value="30" ns1:name="UPGRADE_BOARD_RESTART" ns1:lifeCycle="NORMAL">
        <ns1:desc>Restart ordered to effectuate autoconfiguration of a plug-in unit.
				
				This is only used for PlugInUnit MO.</ns1:desc>
    </ns2:member>
    <ns2:member value="31" ns1:name="OPERATOR_CLASSIFIED_PROBLEMS" ns1:lifeCycle="NORMAL">
        <ns1:desc>Restart ordered to resolve procedural errors or office environmental problems.
				Example: power, grounding, temperature, humidity or security problems.</ns1:desc>
    </ns2:member>
</ns2:EnumDataTypeDefinition>
