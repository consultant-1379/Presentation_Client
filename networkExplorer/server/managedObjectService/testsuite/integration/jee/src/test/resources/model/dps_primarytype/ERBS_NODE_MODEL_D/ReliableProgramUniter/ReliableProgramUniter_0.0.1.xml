<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:PrimaryTypeDefinition xmlns:ns1="urn:com:ericsson:schema:xml:oss:oss_common" xmlns:ns2="urn:com:ericsson:schema:xml:oss:dps_primarytype" xmlns:ns3="urn:com:ericsson:schema:xml:oss:dps_relationship" xmlns:ns4="urn:com:ericsson:schema:xml:oss:cbp_engine" xmlns:ns5="urn:com:ericsson:schema:xml:oss:cbp_common" xmlns:ns6="urn:com:ericsson:schema:xml:oss:cbp_component" xmlns:ns7="urn:com:ericsson:schema:xml:oss:oss_cache" xmlns:ns8="urn:com:ericsson:schema:xml:oss:oss_channel" xmlns:ns9="urn:com:ericsson:schema:xml:oss:oss_edt" xmlns:ns10="urn:com:ericsson:schema:xml:oss:oss_confparam" xmlns:ns11="urn:com:ericsson:schema:xml:oss:cbp_flow" xmlns:ns12="urn:com:ericsson:schema:xml:oss:oss_event" xmlns:ns13="urn:com:ericsson:schema:xml:oss:oss_annotation" xmlns:ns14="urn:com:ericsson:schema:xml:oss:oss_cdt" hierarchyType="HIERARCHICAL" root="false" ns1:ns="ERBS_NODE_MODEL_D" ns1:name="ReliableProgramUniter" ns1:version="0.0.1" ns1:lifeCycle="NORMAL">
    <ns1:modelCreationInfo creationDate="2013-06-26 11:06:21.849" author="mom-xml-transformer-tool">
        <ns1:derivedModel urn="/dps_primarytype/ERBS_NODE_MODEL_D/ERBS_NODE_MODEL_D/0.0.1"/>
    </ns1:modelCreationInfo>
    <ns1:desc>Reliable Program Uniter (RPU)
			
			This is a controlling and addressing entity with one common addressing unit for parts of two Reliable Programs. Each part is called a Resource Suite Handler (RSH). The RSH is implemented and owned by the application. It is not a function or concept within CPP.
			
			Instances of this MO are created automatically during system upgrade. It is also possible to create instances manually, but this is not recommended.</ns1:desc>
    <ns2:primaryTypeAttribute behaviorOnRead="MEDIATION" behaviorOnWrite="MEDIATION" restricted="true" ns1:name="ReliableProgramUniterId" ns1:lifeCycle="NORMAL">
        <ns1:desc>The value component of the RDN.
				
				It is set automatically during system upgrade.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringType"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="false" ns1:name="userLabel" ns1:lifeCycle="NORMAL">
        <ns1:desc>Label for free use.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringType">
            <ns1:stringLengthConstraint>
                <ns1:minMaxRange min="0" max="128"/>
            </ns1:stringLengthConstraint>
        </ns1:type>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="true" ns1:name="reliableProgramLabel" ns1:lifeCycle="NORMAL">
        <ns1:desc>Compare this label to the reliableProgramLabel in the LoadModule MO.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringType"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="false" ns1:name="admActiveSlot" ns1:lifeCycle="NORMAL">
        <ns1:desc>Reference to a Slot, contains the running &quot;Normal&quot; relation to a slot. If the reference is set to the passive slot, the passive slot is cleared and only one relation is set.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:moRefType" modelUrn="/dps_primarytype/ERBS_NODE_MODEL_D/Slot/0.0.1"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="false" ns1:name="admPassiveSlot" ns1:lifeCycle="NORMAL">
        <ns1:desc>Reference to a Slot, contains the running &quot;Stand-by&quot; relation to a slot. If the reference is set to the active slot or a NULL value, the passive slot will be cleared.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:moRefType" modelUrn="/dps_primarytype/ERBS_NODE_MODEL_D/Slot/0.0.1"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="false" ns1:name="switchOver" ns1:lifeCycle="NORMAL">
        <ns1:desc>Controls when to switch the RPU if a certain error occurs.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:enumRefType" modelUrn="/oss_edt/ERBS_NODE_MODEL_D/RPUSwitchOverMode/0.0.1"/>
        <ns1:default xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringValue" value="AT_PIU_FAULT"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="false" ns1:name="normalisation" ns1:lifeCycle="NORMAL">
        <ns1:desc>Controls when to switch back the RPU after error recovery has been done.
				
				Normalisation at PIU unlock is always done, regardless of the value of this attribute.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:enumRefType" modelUrn="/oss_edt/ERBS_NODE_MODEL_D/RPUNormalisationMode/0.0.1"/>
        <ns1:default xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringValue" value="AUTOMATIC"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="true" ns1:name="replication" ns1:lifeCycle="NORMAL">
        <ns1:desc>Specifies the level of data replication that is associated with the RPU.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:enumRefType" modelUrn="/oss_edt/ERBS_NODE_MODEL_D/RPUReplicationMode/0.0.1"/>
        <ns1:default xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringValue" value="APPL_CONTROLLED"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="MEDIATION" behaviorOnWrite="DONT" restricted="false" ns1:name="operationalMode" ns1:lifeCycle="NORMAL">
        <ns1:desc>Specifies if the RSH execution mode (active/passive) in the two Reliable Programs, matches the normal/stand-by relations set on the corresponding RPU.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:enumRefType" modelUrn="/oss_edt/ERBS_NODE_MODEL_D/RPUOperationalMode/0.0.1"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="PERSISTENT_STORAGE" behaviorOnWrite="DONT" restricted="false" ns1:name="reservedBy" ns1:lifeCycle="NORMAL">
        <ns1:desc>Contains a list of MO instances that reserve this MO instance.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:listType">
            <ns1:collectionValuesType xsi:type="ns1:moRefType" modelUrn="/dps_primarytype/ERBS_NODE_MODEL_D/ManagedObject/0.0.1"/>
        </ns1:type>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="PERSISTENT_STORAGE" behaviorOnWrite="DONT" restricted="false" ns1:name="rpuId" ns1:lifeCycle="NORMAL">
        <ns1:desc>The ReilableProgramUniter identity on the resource layer.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longType"/>
    </ns2:primaryTypeAttribute>
</ns2:PrimaryTypeDefinition>
