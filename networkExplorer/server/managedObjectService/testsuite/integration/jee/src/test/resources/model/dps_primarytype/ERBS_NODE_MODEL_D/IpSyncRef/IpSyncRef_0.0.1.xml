<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:PrimaryTypeDefinition xmlns:ns1="urn:com:ericsson:schema:xml:oss:oss_common" xmlns:ns2="urn:com:ericsson:schema:xml:oss:dps_primarytype" xmlns:ns3="urn:com:ericsson:schema:xml:oss:dps_relationship" xmlns:ns4="urn:com:ericsson:schema:xml:oss:cbp_engine" xmlns:ns5="urn:com:ericsson:schema:xml:oss:cbp_common" xmlns:ns6="urn:com:ericsson:schema:xml:oss:cbp_component" xmlns:ns7="urn:com:ericsson:schema:xml:oss:oss_cache" xmlns:ns8="urn:com:ericsson:schema:xml:oss:oss_channel" xmlns:ns9="urn:com:ericsson:schema:xml:oss:oss_edt" xmlns:ns10="urn:com:ericsson:schema:xml:oss:oss_confparam" xmlns:ns11="urn:com:ericsson:schema:xml:oss:cbp_flow" xmlns:ns12="urn:com:ericsson:schema:xml:oss:oss_event" xmlns:ns13="urn:com:ericsson:schema:xml:oss:oss_annotation" xmlns:ns14="urn:com:ericsson:schema:xml:oss:oss_cdt" hierarchyType="HIERARCHICAL" root="false" ns1:ns="ERBS_NODE_MODEL_D" ns1:name="IpSyncRef" ns1:version="0.0.1" ns1:lifeCycle="NORMAL">
    <ns1:modelCreationInfo creationDate="2013-06-26 11:06:21.849" author="mom-xml-transformer-tool">
        <ns1:derivedModel urn="/dps_primarytype/ERBS_NODE_MODEL_D/ERBS_NODE_MODEL_D/0.0.1"/>
    </ns1:modelCreationInfo>
    <ns1:desc>IP Synchronization Reference
			
			Administers the synchronization reference based on time stamps carried by NTP packets.
			
			Refer to the description of the relevant board that supports NTP clients for configuration details.</ns1:desc>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="false" ns1:name="administrativeState" ns1:lifeCycle="NORMAL">
        <ns1:desc>The administrative state.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:enumRefType" modelUrn="/oss_edt/ERBS_NODE_MODEL_D/AdmState/0.0.1"/>
        <ns1:default xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringValue" value="LOCKED"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="MEDIATION" behaviorOnWrite="DONT" restricted="false" ns1:name="availabilityStatus" ns1:lifeCycle="NORMAL">
        <ns1:desc>The availability status.
				
				It contains details about operationalState.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:enumRefType" modelUrn="/oss_edt/ERBS_NODE_MODEL_D/AvailabilityStatus/0.0.1"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="true" ns1:name="IpSyncRefId" ns1:lifeCycle="NORMAL">
        <ns1:desc>The value component of the RDN.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringType"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="false" ns1:name="ntpServerIpAddress" ns1:lifeCycle="NORMAL">
        <ns1:desc>This attribute contains the domain name or the IP address of the NTP server to be used. It must be a valid IPv4 address or a valid domain name.
				
				The input format for an IPv4 address is four fields of digits, separated by a dot. Each field consists of one to three digits.
				
				A valid domain name:
				- has characters of type letters, numbers and a hyphen &quot;-&quot;, where hyphens can be anywhere in the middle of a name, but cannot be located at the end or beginning of a name
				- cannot have spaces
				- is case insensitive
				- can include dots for the use of sub domains.
				
				Two IpSyncRef MOs belonging to the same IpAccessHostEt MO, must have different ntpServerIpAddress values.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringType"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="MEDIATION" behaviorOnWrite="DONT" restricted="false" ns1:name="operationalState" ns1:lifeCycle="NORMAL">
        <ns1:desc>The operational state.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:enumRefType" modelUrn="/oss_edt/ERBS_NODE_MODEL_D/OperState/0.0.1"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="PERSISTENT_STORAGE" behaviorOnWrite="DONT" restricted="false" ns1:name="reservedBy" ns1:lifeCycle="NORMAL">
        <ns1:desc>Contains a list of MO instances that reserve this MO instance.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:listType">
            <ns1:collectionValuesType xsi:type="ns1:moRefType" modelUrn="/dps_primarytype/ERBS_NODE_MODEL_D/ManagedObject/0.0.1"/>
        </ns1:type>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="MEDIATION" behaviorOnWrite="DONT" restricted="false" ns1:name="syncStatus" ns1:lifeCycle="DEPRECATED">
        <ns1:desc>The synchronization status. It reflects both the local and the remote peer of the IP synchronization reference.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:enumRefType" modelUrn="/oss_edt/ERBS_NODE_MODEL_D/SyncStatus/0.0.1"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="PERSISTENT_STORAGE" behaviorOnWrite="DONT" restricted="false" ns1:name="cachedIpAddress" ns1:lifeCycle="NORMAL">
        <ns1:desc>This attribute contains the latest IP address received from the DNS server with the specified domain name. If domain name is not valid or no value yet received from the DNS server this will be a null string.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringType"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="false" ns1:name="userLabel" ns1:lifeCycle="NORMAL">
        <ns1:desc>Label for free use.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringType">
            <ns1:stringLengthConstraint>
                <ns1:minMaxRange min="0" max="128"/>
            </ns1:stringLengthConstraint>
        </ns1:type>
    </ns2:primaryTypeAttribute>
</ns2:PrimaryTypeDefinition>
