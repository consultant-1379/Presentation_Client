<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:PrimaryTypeDefinition xmlns:ns1="urn:com:ericsson:schema:xml:oss:oss_common" xmlns:ns2="urn:com:ericsson:schema:xml:oss:dps_primarytype" xmlns:ns3="urn:com:ericsson:schema:xml:oss:dps_relationship" xmlns:ns4="urn:com:ericsson:schema:xml:oss:cbp_engine" xmlns:ns5="urn:com:ericsson:schema:xml:oss:cbp_common" xmlns:ns6="urn:com:ericsson:schema:xml:oss:cbp_component" xmlns:ns7="urn:com:ericsson:schema:xml:oss:oss_cache" xmlns:ns8="urn:com:ericsson:schema:xml:oss:oss_channel" xmlns:ns9="urn:com:ericsson:schema:xml:oss:oss_edt" xmlns:ns10="urn:com:ericsson:schema:xml:oss:oss_confparam" xmlns:ns11="urn:com:ericsson:schema:xml:oss:cbp_flow" xmlns:ns12="urn:com:ericsson:schema:xml:oss:oss_event" xmlns:ns13="urn:com:ericsson:schema:xml:oss:oss_annotation" xmlns:ns14="urn:com:ericsson:schema:xml:oss:oss_cdt" hierarchyType="HIERARCHICAL" root="false" ns1:ns="ERBS_NODE_MODEL_D" ns1:name="SectorCarrier" ns1:version="0.0.1" ns1:lifeCycle="NORMAL">
    <ns1:modelCreationInfo creationDate="2013-06-26 11:06:21.849" author="mom-xml-transformer-tool">
        <ns1:derivedModel urn="/dps_primarytype/ERBS_NODE_MODEL_D/ERBS_NODE_MODEL_D/0.0.1"/>
    </ns1:modelCreationInfo>
    <ns1:desc>This MO represents the power and frequency usage of the SectorEquipmentFunction that covers an area.</ns1:desc>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="true" ns1:name="SectorCarrierId" ns1:lifeCycle="NORMAL">
        <ns1:desc>The value component of the RDN.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringType"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="MEDIATION" behaviorOnWrite="DONT" restricted="false" ns1:name="availabilityStatus" ns1:lifeCycle="NORMAL">
        <ns1:desc>The availability status. It contains details about operationalState. This attribute is set by the application in the RBS.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:enumRefType" modelUrn="/oss_edt/ERBS_NODE_MODEL_D/AvailabilityStatus/0.0.1"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="MEDIATION" behaviorOnWrite="DONT" restricted="false" ns1:name="operationalState" ns1:lifeCycle="NORMAL">
        <ns1:desc>The operational state. This attribute is set by the application in the RBS.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:enumRefType" modelUrn="/oss_edt/ERBS_NODE_MODEL_D/OperState/0.0.1"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="true" ns1:name="sectorFunctionRef" ns1:lifeCycle="NORMAL">
        <ns1:desc>Reference to an instance of SectorEquipmentFunction MO.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:moRefType" modelUrn="/dps_primarytype/ERBS_NODE_MODEL_D/SectorEquipmentFunction/0.0.1"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="PERSISTENT_STORAGE" behaviorOnWrite="DONT" restricted="false" ns1:name="reservedBy" ns1:lifeCycle="NORMAL">
        <ns1:desc>Contains a list of MO instances that reserve this MO instance.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:listType">
            <ns1:collectionValuesType xsi:type="ns1:moRefType" modelUrn="/dps_primarytype/ERBS_NODE_MODEL_D/ManagedObject/0.0.1"/>
        </ns1:type>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="false" ns1:name="noOfRxAntennas" ns1:lifeCycle="NORMAL">
        <ns1:desc>The number of antennas that can be used for uplink beamforming/MIMO
				A parameter value of 0 means that the available configured resources will be used.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longType">
            <ns1:valueRangeConstraint>
                <ns1:minMaxRange min="0" max="0"/>
                <ns1:minMaxRange min="1" max="1"/>
                <ns1:minMaxRange min="2" max="2"/>
                <ns1:minMaxRange min="4" max="4"/>
                <ns1:minMaxRange min="8" max="8"/>
            </ns1:valueRangeConstraint>
        </ns1:type>
        <ns1:default xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longValue" value="0"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="false" ns1:name="noOfTxAntennas" ns1:lifeCycle="NORMAL">
        <ns1:desc>The number of antennas that can be used for downlink beamforming/MIMO
				A parameter value of 0 means that the available configured resources will be used.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longType">
            <ns1:valueRangeConstraint>
                <ns1:minMaxRange min="0" max="0"/>
                <ns1:minMaxRange min="1" max="1"/>
                <ns1:minMaxRange min="2" max="2"/>
                <ns1:minMaxRange min="4" max="4"/>
                <ns1:minMaxRange min="8" max="8"/>
            </ns1:valueRangeConstraint>
        </ns1:type>
        <ns1:default xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longValue" value="0"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="false" ns1:name="partOfSectorPower" ns1:lifeCycle="NORMAL">
        <ns1:desc>The requested part of the total power in the SectorEquipment that should be allocated for the sectorCarrier. The output power is evenly distributed over the antenna connectors used for TX transmission that are allocated for the SectorCarrier.
				
				If the total amount of power per antenna connector of a SectorEquipment is over 100%  when the cell using the SectorCarrier is unlocked, an alarm will be generated.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longType">
            <ns1:valueRangeConstraint>
                <ns1:minMaxRange min="0" max="100"/>
            </ns1:valueRangeConstraint>
        </ns1:type>
        <ns1:default xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longValue" value="100"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="MEDIATION" behaviorOnWrite="DONT" restricted="false" ns1:name="maximumTransmissionPower" ns1:lifeCycle="NORMAL">
        <ns1:desc>Maximum possible power at the antenna reference point, for all downlink channels in all TX branches used simultaneously in the SectorCarrier.
				-1 is used as an undefined value.
				
				Note: Setting partOfSectorPower or confOutputPower (MO SectorEquipmentFunction) has an impact on this attribute. See Radio Network Configuration user guide 11/1553-HSC 105 50/1.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longType">
            <ns1:valueRangeConstraint>
                <ns1:minMaxRange min="-1" max="540"/>
            </ns1:valueRangeConstraint>
        </ns1:type>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="false" ns1:name="prsEnabled" ns1:lifeCycle="NORMAL">
        <ns1:desc>If true, the Positioning Reference Signal will be sent from this sectorCarrier.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:booleanType"/>
        <ns1:default xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:booleanValue" value="true"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="MEDIATION" behaviorOnWrite="DONT" restricted="false" ns1:name="pmActiveUeUlSumSectorCarrier" ns1:lifeCycle="NORMAL">
        <ns1:desc>Number of UEs considered active in the uplink direction.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longType"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="MEDIATION" behaviorOnWrite="DONT" restricted="false" ns1:name="pmActiveUeDlSumSectorCarrier" ns1:lifeCycle="NORMAL">
        <ns1:desc>Number of UEs considered active in the downlink direction.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longType"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="MEDIATION" behaviorOnWrite="DONT" restricted="false" ns1:name="pmZtemporary56" ns1:lifeCycle="NORMAL">
        <ns1:desc>Normally this counter is not used, it can however sometimes be temporarily used for very late additions in a release. If so, for description see Network Impact Report in theÂ CPI.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longType"/>
    </ns2:primaryTypeAttribute>
</ns2:PrimaryTypeDefinition>
