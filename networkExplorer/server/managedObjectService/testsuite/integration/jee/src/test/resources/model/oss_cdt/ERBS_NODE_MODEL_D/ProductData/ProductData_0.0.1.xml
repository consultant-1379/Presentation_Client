<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:ComplexDataTypeDefinition xmlns:ns1="urn:com:ericsson:schema:xml:oss:oss_common" xmlns:ns2="urn:com:ericsson:schema:xml:oss:oss_cdt" xmlns:ns3="urn:com:ericsson:schema:xml:oss:dps_relationship" xmlns:ns4="urn:com:ericsson:schema:xml:oss:dps_primarytype" xmlns:ns5="urn:com:ericsson:schema:xml:oss:cbp_engine" xmlns:ns6="urn:com:ericsson:schema:xml:oss:cbp_common" xmlns:ns7="urn:com:ericsson:schema:xml:oss:cbp_component" xmlns:ns8="urn:com:ericsson:schema:xml:oss:oss_cache" xmlns:ns9="urn:com:ericsson:schema:xml:oss:oss_channel" xmlns:ns10="urn:com:ericsson:schema:xml:oss:oss_edt" xmlns:ns11="urn:com:ericsson:schema:xml:oss:oss_confparam" xmlns:ns12="urn:com:ericsson:schema:xml:oss:cbp_flow" xmlns:ns13="urn:com:ericsson:schema:xml:oss:oss_event" xmlns:ns14="urn:com:ericsson:schema:xml:oss:oss_annotation" ns1:ns="ERBS_NODE_MODEL_D" ns1:name="ProductData" ns1:version="0.0.1" ns1:lifeCycle="NORMAL" ns1:lifeCycleDesc="">
    <ns1:modelCreationInfo creationDate="2013-06-26 11:06:21.849" author="mom-xml-transformer-tool">
        <ns1:derivedModel urn="/oss_cdt/ERBS_NODE_MODEL_D/ERBS_NODE_MODEL_D/0.0.1"/>
    </ns1:modelCreationInfo>
    <ns1:desc>ProductData</ns1:desc>
    <ns2:attribute ns1:name="productionDate" ns1:lifeCycle="NORMAL">
        <ns1:desc>The production date is represented in two ways:
				
				- the preferred syntax is four-digits for the year, two digits for the month, and two digits for the day, a total of eight characters.
				Example: 19991231
				
				- the date is also be represented by two digits for the year, followed by the letter W for week, and two digits for the number of the week, a total of five characters.
				Example: 99W52</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringType">
            <ns1:stringLengthConstraint>
                <ns1:minMaxRange min="0" max="8"/>
            </ns1:stringLengthConstraint>
        </ns1:type>
    </ns2:attribute>
    <ns2:attribute ns1:name="productName" ns1:lifeCycle="NORMAL">
        <ns1:desc>The name of the product.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringType">
            <ns1:stringLengthConstraint>
                <ns1:minMaxRange min="0" max="12"/>
            </ns1:stringLengthConstraint>
        </ns1:type>
    </ns2:attribute>
    <ns2:attribute ns1:name="productNumber" ns1:lifeCycle="NORMAL">
        <ns1:desc>The product number of the product.
				
				Examples: ABC 101 001, CXC 123 456, 1/BFD 101 999</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringType">
            <ns1:stringLengthConstraint>
                <ns1:minMaxRange min="0" max="24"/>
            </ns1:stringLengthConstraint>
        </ns1:type>
    </ns2:attribute>
    <ns2:attribute ns1:name="productRevision" ns1:lifeCycle="NORMAL">
        <ns1:desc>The revision state of the product.
				
				Examples: R1, R1A, R1A02, P7B</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringType">
            <ns1:stringLengthConstraint>
                <ns1:minMaxRange min="0" max="7"/>
            </ns1:stringLengthConstraint>
        </ns1:type>
    </ns2:attribute>
    <ns2:attribute ns1:name="serialNumber" ns1:lifeCycle="NORMAL">
        <ns1:desc>The serial number of the product.
				
				The serial number consists of up to 13 alphanumerical characters.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringType">
            <ns1:stringLengthConstraint>
                <ns1:minMaxRange min="0" max="13"/>
            </ns1:stringLengthConstraint>
        </ns1:type>
    </ns2:attribute>
</ns2:ComplexDataTypeDefinition>
