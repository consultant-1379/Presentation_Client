<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:PrimaryTypeDefinition xmlns:ns1="urn:com:ericsson:schema:xml:oss:oss_common" xmlns:ns2="urn:com:ericsson:schema:xml:oss:dps_primarytype" xmlns:ns3="urn:com:ericsson:schema:xml:oss:dps_relationship" xmlns:ns4="urn:com:ericsson:schema:xml:oss:cbp_engine" xmlns:ns5="urn:com:ericsson:schema:xml:oss:cbp_common" xmlns:ns6="urn:com:ericsson:schema:xml:oss:cbp_component" xmlns:ns7="urn:com:ericsson:schema:xml:oss:oss_cache" xmlns:ns8="urn:com:ericsson:schema:xml:oss:oss_channel" xmlns:ns9="urn:com:ericsson:schema:xml:oss:oss_edt" xmlns:ns10="urn:com:ericsson:schema:xml:oss:oss_confparam" xmlns:ns11="urn:com:ericsson:schema:xml:oss:cbp_flow" xmlns:ns12="urn:com:ericsson:schema:xml:oss:oss_event" xmlns:ns13="urn:com:ericsson:schema:xml:oss:oss_annotation" xmlns:ns14="urn:com:ericsson:schema:xml:oss:oss_cdt" hierarchyType="HIERARCHICAL" root="false" ns1:ns="ERBS_NODE_MODEL_D" ns1:name="PlugInUnit" ns1:version="0.0.1" ns1:lifeCycle="NORMAL">
    <ns1:modelCreationInfo creationDate="2013-06-26 11:06:21.849" author="mom-xml-transformer-tool">
        <ns1:derivedModel urn="/dps_primarytype/ERBS_NODE_MODEL_D/ERBS_NODE_MODEL_D/0.0.1"/>
    </ns1:modelCreationInfo>
    <ns1:desc>Plug-In Unit
			
			This MO represents a plug-in unit, that is a board inserted in a slot in a subrack.
			
			There are two mutually exclusively sets of LED attributes, depending on the version of the system. The sets are (greenLed, redLed, yellowLed) and (faultIndicator, operationalIndicator, maintenanceIndicator). The set not in use will display the value NOT_APPLICABLE for its attributes.</ns1:desc>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="true" ns1:name="PlugInUnitId" ns1:lifeCycle="NORMAL">
        <ns1:desc>The value component of the RDN.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringType"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="false" ns1:name="userLabel" ns1:lifeCycle="NORMAL">
        <ns1:desc>Label for free use.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringType">
            <ns1:stringLengthConstraint>
                <ns1:minMaxRange min="0" max="128"/>
            </ns1:stringLengthConstraint>
        </ns1:type>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="false" ns1:name="administrativeState" ns1:lifeCycle="NORMAL">
        <ns1:desc>The administrative state.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:enumRefType" modelUrn="/oss_edt/ERBS_NODE_MODEL_D/AdmState/0.0.1"/>
        <ns1:default xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringValue" value="LOCKED"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="MEDIATION" behaviorOnWrite="DONT" restricted="false" ns1:name="operationalState" ns1:lifeCycle="NORMAL">
        <ns1:desc>The operational state.
				
				It is set to ENABLED when the basic OS is started, all other programs on the MP/BP are loaded/started, and all hardware tests during start-up are performed.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:enumRefType" modelUrn="/oss_edt/ERBS_NODE_MODEL_D/OperState/0.0.1"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="PERSISTENT_STORAGE" behaviorOnWrite="DONT" restricted="false" ns1:name="productType" ns1:lifeCycle="NORMAL">
        <ns1:desc>Describes the product type.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringType"/>
        <ns1:default xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringValue" value="HW"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="true" ns1:name="piuType" ns1:lifeCycle="NORMAL">
        <ns1:desc>Reference to an instance of PiuType.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:moRefType" modelUrn="/dps_primarytype/ERBS_NODE_MODEL_D/PiuType/0.0.1"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="false" ns1:name="piuGroupNumber" ns1:lifeCycle="NORMAL">
        <ns1:desc>Indicates which restart group the plug-in unit belongs to.
				
				A restart group consists of a number of plug-in units, where at least one must be enabled (working) or performing recovery. When no plug-in unit in a group is working, the node is restarted automatically.
				
				Must have a positive value.
				
				Zero indicates no group.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longType">
            <ns1:valueRangeConstraint>
                <ns1:minMaxRange min="0" max="65536"/>
            </ns1:valueRangeConstraint>
        </ns1:type>
        <ns1:default xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longValue" value="0"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="MEDIATION" behaviorOnWrite="DONT" restricted="false" ns1:name="availabilityStatus" ns1:lifeCycle="NORMAL">
        <ns1:desc>The availability status.
				
				It contains details about operationalState.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:enumRefType" modelUrn="/oss_edt/ERBS_NODE_MODEL_D/AvailabilityStatus/0.0.1"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="PERSISTENT_STORAGE" behaviorOnWrite="DONT" restricted="false" ns1:name="reservedBy" ns1:lifeCycle="NORMAL">
        <ns1:desc>Contains a list of MO instances that reserve this MO instance.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:listType">
            <ns1:collectionValuesType xsi:type="ns1:moRefType" modelUrn="/dps_primarytype/ERBS_NODE_MODEL_D/ManagedObject/0.0.1"/>
        </ns1:type>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="MEDIATION" behaviorOnWrite="DONT" restricted="false" ns1:name="pmProcessorLoad" ns1:lifeCycle="NORMAL">
        <ns1:desc>The average CPU load on the whole system (all the cores) during the last 5 minutes.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longType"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="false" ns1:name="allowedSeqRestarts" ns1:lifeCycle="NORMAL">
        <ns1:desc>The number of automatic sequential restarts that are allowed for a PIU. When this number of automatic restarts has been exceeded, the automatic restarting halts and the PIU is fault-marked.
				
				The time limit between two restarts is 5 minutes. This means that if the time between two restarts is less than 5 minutes, the restarts are counted, otherwise the restart counter is reset.
				
				For example, RESTARTS_WITH_WARM means 3 restarts with time limit less than 5 minutes are accepted but when the 4th occurs within 5 minutes, the board is considered faulty and an alarm is issued. The attribute specifies the escalation staircase for automatic (spontanenous) restarts, for the board. Example: if the attribute it set to RESTARTS_WITH_COLD, the first restart will be of rank cold, the second (within five minutes) will be cold with test. The third restart (within five minutes) will fault-mark the board.
				
				If no value is specified when the MO is created, a default value, fetched from defAllowedSeqRestarts in the referenced MO, PiuType, is used.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:enumRefType" modelUrn="/oss_edt/ERBS_NODE_MODEL_D/SeqRestarts/0.0.1"/>
        <ns1:default xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringValue" value="RESTARTS_WITH_WARM"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="PERSISTENT_STORAGE" behaviorOnWrite="DONT" restricted="false" ns1:name="hwTestResult" ns1:lifeCycle="NORMAL">
        <ns1:desc>Contains text information from latest performed hardware tests.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringType"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="PERSISTENT_STORAGE" behaviorOnWrite="DONT" restricted="false" ns1:name="hwTestStatus" ns1:lifeCycle="NORMAL">
        <ns1:desc>Indicates the status of hardware tests.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:enumRefType" modelUrn="/oss_edt/ERBS_NODE_MODEL_D/TestStatus/0.0.1"/>
        <ns1:default xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringValue" value="NO_TEST_RESULTS"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="MEDIATION" behaviorOnWrite="DONT" restricted="false" ns1:name="greenLed" ns1:lifeCycle="NORMAL">
        <ns1:desc>Indicates the operational status of the plug-in unit.
				
				The following values are possible for this attribute:
				
				NOT_APPLICABLE = The LED is not supported
				NOT_AVAILABLE = The LED state is not readable at the moment
				STEADY_LIGHT = Power present
				SLOW_BLINK = Missing resource
				FAST_BLINK = Load or test in progress
				FLICKERING = Busy
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:enumRefType" modelUrn="/oss_edt/ERBS_NODE_MODEL_D/LedStatus/0.0.1"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="MEDIATION" behaviorOnWrite="DONT" restricted="false" ns1:name="redLed" ns1:lifeCycle="NORMAL">
        <ns1:desc>Indicates the fault status of the plug-in unit.
				
				The following values are possible for this attribute:
				
				NOT_APPLICABLE =The LED is not supported
				NOT_AVAILABLE = The LED state is not available at the moment
				OUT = No fault
				STEADY_LIGHT = PIU fault
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:enumRefType" modelUrn="/oss_edt/ERBS_NODE_MODEL_D/LedStatus/0.0.1"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="MEDIATION" behaviorOnWrite="DONT" restricted="false" ns1:name="yellowLed" ns1:lifeCycle="NORMAL">
        <ns1:desc>Indicates the information status of the plug-in unit.
				
				The following values are possible for this attribute:
				
				NOT_APPLICABLE =The LED is not supported
				NOT_AVAILABLE = The LED state is not available at the moment
				OUT = PIU unlocked
				STEADY_LIGHT = PIU locked
				SLOW_BLINK = Shutting-down in progress
				FLICKERING = Busy
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:enumRefType" modelUrn="/oss_edt/ERBS_NODE_MODEL_D/LedStatus/0.0.1"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="MEDIATION" behaviorOnWrite="DONT" restricted="false" ns1:name="unitType" ns1:lifeCycle="NORMAL">
        <ns1:desc>Displays the same value as the attribute productName from the struct AdminProductData in MO PiuType.
				</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringType">
            <ns1:stringLengthConstraint>
                <ns1:minMaxRange min="1" max="12"/>
            </ns1:stringLengthConstraint>
        </ns1:type>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="MEDIATION" behaviorOnWrite="DONT" restricted="false" ns1:name="maintenanceIndicator" ns1:lifeCycle="NORMAL">
        <ns1:desc>Indicates the behavior of the Maintenance indicator.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:enumRefType" modelUrn="/oss_edt/ERBS_NODE_MODEL_D/IndicatorStatus/0.0.1"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="MEDIATION" behaviorOnWrite="DONT" restricted="false" ns1:name="faultIndicator" ns1:lifeCycle="NORMAL">
        <ns1:desc>Indicates the behavior of the Fault indicator.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:enumRefType" modelUrn="/oss_edt/ERBS_NODE_MODEL_D/IndicatorStatus/0.0.1"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="MEDIATION" behaviorOnWrite="DONT" restricted="false" ns1:name="operationalIndicator" ns1:lifeCycle="NORMAL">
        <ns1:desc>Indicates the behavior of the Operational indicator.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:enumRefType" modelUrn="/oss_edt/ERBS_NODE_MODEL_D/IndicatorStatus/0.0.1"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="MEDIATION" behaviorOnWrite="DONT" restricted="false" ns1:name="statusIndicator" ns1:lifeCycle="NORMAL">
        <ns1:desc>Indicates the behavior of the Status indicator.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:enumRefType" modelUrn="/oss_edt/ERBS_NODE_MODEL_D/IndicatorStatus/0.0.1"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="MEDIATION" behaviorOnWrite="DONT" restricted="false" ns1:name="pmProcessorCoreLoad" ns1:lifeCycle="NORMAL">
        <ns1:desc>The average CPU load on each processor core during the whole ROP period.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:listType">
            <ns1:collectionSizeConstraint>
                <ns1:maxValue value="32"/>
            </ns1:collectionSizeConstraint>
            <ns1:collectionValuesType xsi:type="ns1:longType"/>
        </ns1:type>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="MEDIATION" behaviorOnWrite="DONT" restricted="false" ns1:name="pmSwitchIngressDiscards" ns1:lifeCycle="NORMAL">
        <ns1:desc>Total number of cells discarded due to overload in the ingress direction.
				
				This counter is not supported on all boards, see the description of the relevant board.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longType"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="MEDIATION" behaviorOnWrite="DONT" restricted="false" ns1:name="pmSwitchIngressLoad" ns1:lifeCycle="NORMAL">
        <ns1:desc>Average ingress load on the board.
				
				Maximum load depends on the hardware version (DBM). This counter is not supported on all boards, see the description of the relevant board.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longType"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="PERSISTENT_STORAGE" behaviorOnWrite="DONT" restricted="false" ns1:name="signatureValidationService" ns1:lifeCycle="NORMAL">
        <ns1:desc>Indicates if the Signed Software (SSW) function is enabled or disabled.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:enumRefType" modelUrn="/oss_edt/ERBS_NODE_MODEL_D/SignatureValidationService/0.0.1"/>
    </ns2:primaryTypeAttribute>
</ns2:PrimaryTypeDefinition>
