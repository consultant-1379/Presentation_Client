<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:PrimaryTypeDefinition xmlns:ns1="urn:com:ericsson:schema:xml:oss:oss_common" xmlns:ns2="urn:com:ericsson:schema:xml:oss:dps_primarytype" xmlns:ns3="urn:com:ericsson:schema:xml:oss:dps_relationship" xmlns:ns4="urn:com:ericsson:schema:xml:oss:cbp_engine" xmlns:ns5="urn:com:ericsson:schema:xml:oss:cbp_common" xmlns:ns6="urn:com:ericsson:schema:xml:oss:cbp_component" xmlns:ns7="urn:com:ericsson:schema:xml:oss:oss_cache" xmlns:ns8="urn:com:ericsson:schema:xml:oss:oss_channel" xmlns:ns9="urn:com:ericsson:schema:xml:oss:oss_edt" xmlns:ns10="urn:com:ericsson:schema:xml:oss:oss_confparam" xmlns:ns11="urn:com:ericsson:schema:xml:oss:cbp_flow" xmlns:ns12="urn:com:ericsson:schema:xml:oss:oss_event" xmlns:ns13="urn:com:ericsson:schema:xml:oss:oss_annotation" xmlns:ns14="urn:com:ericsson:schema:xml:oss:oss_cdt" hierarchyType="HIERARCHICAL" root="false" ns1:ns="ERBS_NODE_MODEL_D" ns1:name="LoadBalancingFunction" ns1:version="0.0.1" ns1:lifeCycle="NORMAL">
    <ns1:modelCreationInfo creationDate="2013-06-26 11:06:21.849" author="mom-xml-transformer-tool">
        <ns1:derivedModel urn="/dps_primarytype/ERBS_NODE_MODEL_D/ERBS_NODE_MODEL_D/0.0.1"/>
    </ns1:modelCreationInfo>
    <ns1:desc>Contains parameters and constraints for Idle Mode Load Balancing functions</ns1:desc>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="false" ns1:name="threshServingLowHystMin" ns1:lifeCycle="DEPRECATED">
        <ns1:desc>Specifies the minimum hysteresis between the SIB3:threshServingLow and EUtranFreqReleation:threshXHigh for a High Priority Cell (HPC) when used in load balancing function, otherwise ignored.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longType">
            <ns1:valueRangeConstraint>
                <ns1:minMaxRange min="0" max="62"/>
            </ns1:valueRangeConstraint>
            <ns1:valueResolutionConstraint value="2"/>
        </ns1:type>
        <ns1:default xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longValue" value="2"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="false" ns1:name="threshServingLowMin" ns1:lifeCycle="DEPRECATED">
        <ns1:desc>Specifies the lower bound for the SIB3:threshServingLow parameter. Only used by the High Priority Cell (HPC) for idle load balancing function, otherwise ignored.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longType">
            <ns1:valueRangeConstraint>
                <ns1:minMaxRange min="0" max="62"/>
            </ns1:valueRangeConstraint>
            <ns1:valueResolutionConstraint value="2"/>
        </ns1:type>
        <ns1:default xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longValue" value="0"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="false" ns1:name="threshXHighMax" ns1:lifeCycle="DEPRECATED">
        <ns1:desc>Specifies the upper bound for the EUtranFreqReleation:threshXHigh parameter. Only used by the Low Priority Cell (LPC) for idle load balancing function, otherwise ignored.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longType">
            <ns1:valueRangeConstraint>
                <ns1:minMaxRange min="0" max="62"/>
            </ns1:valueRangeConstraint>
            <ns1:valueResolutionConstraint value="2"/>
        </ns1:type>
        <ns1:default xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longValue" value="62"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="false" ns1:name="threshXHighHystMin" ns1:lifeCycle="DEPRECATED">
        <ns1:desc>Specifies the minimum hysteresis between the EUtranFreqReleation:threshXHigh and SIB3:threshServingLow for a Low Priority Cell (LPC) when used in load balancing function, otherwise ignored.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longType">
            <ns1:valueRangeConstraint>
                <ns1:minMaxRange min="0" max="62"/>
            </ns1:valueRangeConstraint>
            <ns1:valueResolutionConstraint value="2"/>
        </ns1:type>
        <ns1:default xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longValue" value="2"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="true" ns1:name="LoadBalancingFunctionId" ns1:lifeCycle="NORMAL">
        <ns1:desc>The value component of the RDN.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringType"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="MEDIATION" behaviorOnWrite="DONT" restricted="false" ns1:name="pmAvSubRatioSum" ns1:lifeCycle="DEPRECATED">
        <ns1:desc>Recording the subscription ratio as the result of the traffic load assessment at every sample rate. Those values are accumulated each ROP, allowing the average subscription ratio to be presented for each period.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longType"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="MEDIATION" behaviorOnWrite="DONT" restricted="false" ns1:name="pmAvSubRatioSamp" ns1:lifeCycle="DEPRECATED">
        <ns1:desc>Describes how many samplings that have been performed during the ROP period. This together with the sum counter can be used to calculate an average subscription ratio.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longType"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="false" ns1:name="lbThreshold" ns1:lifeCycle="NORMAL">
        <ns1:desc>The minimum load (subscription ratio) difference to trigger load balancing action.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longType">
            <ns1:valueRangeConstraint>
                <ns1:minMaxRange min="0" max="1000"/>
            </ns1:valueRangeConstraint>
        </ns1:type>
        <ns1:default xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longValue" value="30"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="false" ns1:name="lbCeiling" ns1:lifeCycle="NORMAL">
        <ns1:desc>The maximum load (subscription ratio) difference taken into account in load balancing action.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longType">
            <ns1:valueRangeConstraint>
                <ns1:minMaxRange min="10" max="1000"/>
            </ns1:valueRangeConstraint>
        </ns1:type>
        <ns1:default xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longValue" value="200"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="false" ns1:name="lbUtranOffloadBackoffTime" ns1:lifeCycle="NORMAL">
        <ns1:desc>Backoff time for eNodeB
				The value applies to inhibit further offload to a UTRAN target cell that rejects incoming offload.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longType">
            <ns1:valueRangeConstraint>
                <ns1:minMaxRange min="30" max="600"/>
            </ns1:valueRangeConstraint>
            <ns1:valueResolutionConstraint value="15"/>
        </ns1:type>
        <ns1:default xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longValue" value="60"/>
    </ns2:primaryTypeAttribute>
    <ns2:primaryTypeAttribute behaviorOnRead="INHERITED" behaviorOnWrite="INHERITED" restricted="false" ns1:name="mbmsDisabledLoadBalancing" ns1:lifeCycle="NORMAL">
        <ns1:desc>Parameter used to specify if load balancing action from a cell shall be disabled due to MBMS. If mbmsDisabledLoadBalancing is TRUE and if mbsfnAreaCellStatus is ACTIVATED for a cell, then load balancing action from the cell is disabled.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:booleanType"/>
        <ns1:default xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:booleanValue" value="false"/>
    </ns2:primaryTypeAttribute>
</ns2:PrimaryTypeDefinition>
