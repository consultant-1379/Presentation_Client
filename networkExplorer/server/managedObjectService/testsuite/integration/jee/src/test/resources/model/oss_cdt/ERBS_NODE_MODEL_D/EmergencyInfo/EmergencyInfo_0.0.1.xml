<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:ComplexDataTypeDefinition xmlns:ns1="urn:com:ericsson:schema:xml:oss:oss_common" xmlns:ns2="urn:com:ericsson:schema:xml:oss:oss_cdt" xmlns:ns3="urn:com:ericsson:schema:xml:oss:dps_relationship" xmlns:ns4="urn:com:ericsson:schema:xml:oss:dps_primarytype" xmlns:ns5="urn:com:ericsson:schema:xml:oss:cbp_engine" xmlns:ns6="urn:com:ericsson:schema:xml:oss:cbp_common" xmlns:ns7="urn:com:ericsson:schema:xml:oss:cbp_component" xmlns:ns8="urn:com:ericsson:schema:xml:oss:oss_cache" xmlns:ns9="urn:com:ericsson:schema:xml:oss:oss_channel" xmlns:ns10="urn:com:ericsson:schema:xml:oss:oss_edt" xmlns:ns11="urn:com:ericsson:schema:xml:oss:oss_confparam" xmlns:ns12="urn:com:ericsson:schema:xml:oss:cbp_flow" xmlns:ns13="urn:com:ericsson:schema:xml:oss:oss_event" xmlns:ns14="urn:com:ericsson:schema:xml:oss:oss_annotation" ns1:ns="ERBS_NODE_MODEL_D" ns1:name="EmergencyInfo" ns1:version="0.0.1" ns1:lifeCycle="NORMAL" ns1:lifeCycleDesc="">
    <ns1:modelCreationInfo creationDate="2013-06-26 11:06:21.849" author="mom-xml-transformer-tool">
        <ns1:derivedModel urn="/oss_cdt/ERBS_NODE_MODEL_D/ERBS_NODE_MODEL_D/0.0.1"/>
    </ns1:modelCreationInfo>
    <ns1:desc>This attribute provides information about the current EmergencyState.
			
			For the states NEVER_USED and USE_DISABLED the return value for time is insignificant.</ns1:desc>
    <ns2:attribute ns1:name="state" ns1:lifeCycle="NORMAL">
        <ns1:desc>The current emergency state of the license server.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:enumRefType" modelUrn="/oss_edt/ERBS_NODE_MODEL_D/EmergencyStatus/0.0.1"/>
        <ns1:default xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringValue" value="NEVER_USED"/>
    </ns2:attribute>
    <ns2:attribute ns1:name="time" ns1:lifeCycle="NORMAL">
        <ns1:desc>Indicates the remaining time in state ACTIVE or ACTIVE_AGAIN.
				
				The time out values for the states ACTIVE is 7 days and for ACTIVE_AGAIN is varies from 14 to 7 days.
				
				The time out value is 0 when the state is NEVER_USED, USE_DEGRADED and USE_DISABLED.
				
				1 day = 86400 s
				7 days = 604800 s
				14 days = 1 209 600 s</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longType"/>
        <ns1:default xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:longValue" value="0"/>
    </ns2:attribute>
    <ns2:attribute ns1:name="expiryTime" ns1:lifeCycle="NORMAL">
        <ns1:desc>Indicates the expiry time as a string with the following format &quot;yyyy-mm-dd hh:mm:ss&quot;.
				
				The expiry time value is empty when the state is NEVER_USED, USE_DEGRADED and USE_DISABLED.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringType"/>
    </ns2:attribute>
</ns2:ComplexDataTypeDefinition>
