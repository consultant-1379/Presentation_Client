<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:ComplexDataTypeDefinition xmlns:ns1="urn:com:ericsson:schema:xml:oss:oss_common" xmlns:ns2="urn:com:ericsson:schema:xml:oss:oss_cdt" xmlns:ns3="urn:com:ericsson:schema:xml:oss:dps_relationship" xmlns:ns4="urn:com:ericsson:schema:xml:oss:dps_primarytype" xmlns:ns5="urn:com:ericsson:schema:xml:oss:cbp_engine" xmlns:ns6="urn:com:ericsson:schema:xml:oss:cbp_common" xmlns:ns7="urn:com:ericsson:schema:xml:oss:cbp_component" xmlns:ns8="urn:com:ericsson:schema:xml:oss:oss_cache" xmlns:ns9="urn:com:ericsson:schema:xml:oss:oss_channel" xmlns:ns10="urn:com:ericsson:schema:xml:oss:oss_edt" xmlns:ns11="urn:com:ericsson:schema:xml:oss:oss_confparam" xmlns:ns12="urn:com:ericsson:schema:xml:oss:cbp_flow" xmlns:ns13="urn:com:ericsson:schema:xml:oss:oss_event" xmlns:ns14="urn:com:ericsson:schema:xml:oss:oss_annotation" ns1:ns="ERBS_NODE_MODEL_D" ns1:name="OperFaultEntry" ns1:version="0.0.1" ns1:lifeCycle="NORMAL" ns1:lifeCycleDesc="">
    <ns1:modelCreationInfo creationDate="2013-06-26 11:06:21.849" author="mom-xml-transformer-tool">
        <ns1:derivedModel urn="/oss_cdt/ERBS_NODE_MODEL_D/ERBS_NODE_MODEL_D/0.0.1"/>
    </ns1:modelCreationInfo>
    <ns1:desc>OperFaultEntry</ns1:desc>
    <ns2:attribute ns1:name="countryCode" ns1:lifeCycle="NORMAL">
        <ns1:desc>Two letter country code.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringType">
            <ns1:stringLengthConstraint>
                <ns1:minMaxRange min="2" max="2"/>
            </ns1:stringLengthConstraint>
        </ns1:type>
    </ns2:attribute>
    <ns2:attribute ns1:name="descriptionOfFault" ns1:lifeCycle="NORMAL">
        <ns1:desc>Specific description of the fault observed. It is also important that other external factors are described as well. For product safety reasons also specify fire, water damage, and so on.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringType">
            <ns1:stringLengthConstraint>
                <ns1:minMaxRange min="1" max="64"/>
            </ns1:stringLengthConstraint>
        </ns1:type>
    </ns2:attribute>
    <ns2:attribute ns1:name="preparedBy" ns1:lifeCycle="NORMAL">
        <ns1:desc>Name of the person preparing the log entry.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringType">
            <ns1:stringLengthConstraint>
                <ns1:minMaxRange min="1" max="15"/>
            </ns1:stringLengthConstraint>
        </ns1:type>
    </ns2:attribute>
    <ns2:attribute ns1:name="reference" ns1:lifeCycle="NORMAL">
        <ns1:desc>Compliant reference.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringType">
            <ns1:stringLengthConstraint>
                <ns1:minMaxRange min="0" max="8"/>
            </ns1:stringLengthConstraint>
        </ns1:type>
    </ns2:attribute>
    <ns2:attribute ns1:name="stateCode" ns1:lifeCycle="NORMAL">
        <ns1:desc>The HW status when the failure occurred:
				
				R = repaired unit failed during installation or test
				S = unit in service failure occurred
				T = new unit failed during installation or test</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringType">
            <ns1:stringLengthConstraint>
                <ns1:minMaxRange min="1" max="1"/>
            </ns1:stringLengthConstraint>
        </ns1:type>
    </ns2:attribute>
    <ns2:attribute ns1:name="timeDate" ns1:lifeCycle="NORMAL">
        <ns1:desc>Time stamp date and time as yymmdd hhmmss.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringType">
            <ns1:stringLengthConstraint>
                <ns1:minMaxRange min="13" max="13"/>
            </ns1:stringLengthConstraint>
        </ns1:type>
    </ns2:attribute>
    <ns2:attribute ns1:name="trNumber" ns1:lifeCycle="NORMAL">
        <ns1:desc>If the board has a connection with a trouble report, the trNumber must be supplied.</ns1:desc>
        <ns1:type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="ns1:stringType">
            <ns1:stringLengthConstraint>
                <ns1:minMaxRange min="0" max="10"/>
            </ns1:stringLengthConstraint>
        </ns1:type>
    </ns2:attribute>
</ns2:ComplexDataTypeDefinition>
