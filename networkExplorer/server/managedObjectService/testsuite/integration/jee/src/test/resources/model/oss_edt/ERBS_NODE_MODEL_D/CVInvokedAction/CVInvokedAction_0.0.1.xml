<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:EnumDataTypeDefinition xmlns:ns1="urn:com:ericsson:schema:xml:oss:oss_common" xmlns:ns2="urn:com:ericsson:schema:xml:oss:oss_edt" xmlns:ns3="urn:com:ericsson:schema:xml:oss:dps_primarytype" xmlns:ns4="urn:com:ericsson:schema:xml:oss:cbp_engine" xmlns:ns5="urn:com:ericsson:schema:xml:oss:cbp_common" xmlns:ns6="urn:com:ericsson:schema:xml:oss:cbp_component" xmlns:ns7="urn:com:ericsson:schema:xml:oss:dps_relationship" xmlns:ns8="urn:com:ericsson:schema:xml:oss:oss_cache" xmlns:ns9="urn:com:ericsson:schema:xml:oss:oss_channel" xmlns:ns10="urn:com:ericsson:schema:xml:oss:oss_confparam" xmlns:ns11="urn:com:ericsson:schema:xml:oss:cbp_flow" xmlns:ns12="urn:com:ericsson:schema:xml:oss:oss_event" xmlns:ns13="urn:com:ericsson:schema:xml:oss:oss_annotation" xmlns:ns14="urn:com:ericsson:schema:xml:oss:oss_cdt" ns1:ns="ERBS_NODE_MODEL_D" ns1:name="CVInvokedAction" ns1:version="0.0.1" ns1:lifeCycle="NORMAL">
    <ns1:modelCreationInfo creationDate="2013-06-26 11:06:21.849" author="mom-xml-transformer-tool">
        <ns1:derivedModel urn="/oss_edt/ERBS_NODE_MODEL_D/ERBS_NODE_MODEL_D/0.0.1"/>
    </ns1:modelCreationInfo>
    <ns1:desc>Configuration version invoked action</ns1:desc>
    <ns2:member value="0" ns1:name="CONFIRM_RESTORE" ns1:lifeCycle="NORMAL">
        <ns1:desc>Makes the restore permanent.</ns1:desc>
    </ns2:member>
    <ns2:member value="1" ns1:name="FORCED_RESTORE" ns1:lifeCycle="NORMAL">
        <ns1:desc>Restores the node to a previously downloaded backup CV irrespectively of warnings are detected or not during the verification phase.</ns1:desc>
    </ns2:member>
    <ns2:member value="2" ns1:name="GET_FROM_FTP_SERVER" ns1:lifeCycle="NORMAL">
        <ns1:desc>Downloads a remote backup of a CV to the node.</ns1:desc>
    </ns2:member>
    <ns2:member value="3" ns1:name="PUT_TO_FTP_SERVER" ns1:lifeCycle="NORMAL">
        <ns1:desc>Creates a backup of a CV existing on the node and uploads it to an FTP server.</ns1:desc>
    </ns2:member>
    <ns2:member value="4" ns1:name="RESTORE" ns1:lifeCycle="NORMAL">
        <ns1:desc>Restores the node to a previously downloaded backup CV.</ns1:desc>
    </ns2:member>
    <ns2:member value="5" ns1:name="VERIFY_RESTORE" ns1:lifeCycle="NORMAL">
        <ns1:desc>Verifies that restore of a downloaded backup CV is possible.</ns1:desc>
    </ns2:member>
    <ns2:member value="6" ns1:name="ACTIVATE_ROBUST_RECONFIGURATION" ns1:lifeCycle="NORMAL">
        <ns1:desc>Activates the robust reconfiguration function.</ns1:desc>
    </ns2:member>
    <ns2:member value="7" ns1:name="DEACTIVATE_ROBUST_RECONFIGURATION" ns1:lifeCycle="NORMAL">
        <ns1:desc>Deactivates the robust reconfiguration function.</ns1:desc>
    </ns2:member>
</ns2:EnumDataTypeDefinition>
