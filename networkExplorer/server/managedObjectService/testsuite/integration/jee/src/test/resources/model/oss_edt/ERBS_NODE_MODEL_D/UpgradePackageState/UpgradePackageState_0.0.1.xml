<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ns2:EnumDataTypeDefinition xmlns:ns1="urn:com:ericsson:schema:xml:oss:oss_common" xmlns:ns2="urn:com:ericsson:schema:xml:oss:oss_edt" xmlns:ns3="urn:com:ericsson:schema:xml:oss:dps_primarytype" xmlns:ns4="urn:com:ericsson:schema:xml:oss:cbp_engine" xmlns:ns5="urn:com:ericsson:schema:xml:oss:cbp_common" xmlns:ns6="urn:com:ericsson:schema:xml:oss:cbp_component" xmlns:ns7="urn:com:ericsson:schema:xml:oss:dps_relationship" xmlns:ns8="urn:com:ericsson:schema:xml:oss:oss_cache" xmlns:ns9="urn:com:ericsson:schema:xml:oss:oss_channel" xmlns:ns10="urn:com:ericsson:schema:xml:oss:oss_confparam" xmlns:ns11="urn:com:ericsson:schema:xml:oss:cbp_flow" xmlns:ns12="urn:com:ericsson:schema:xml:oss:oss_event" xmlns:ns13="urn:com:ericsson:schema:xml:oss:oss_annotation" xmlns:ns14="urn:com:ericsson:schema:xml:oss:oss_cdt" ns1:ns="ERBS_NODE_MODEL_D" ns1:name="UpgradePackageState" ns1:version="0.0.1" ns1:lifeCycle="NORMAL">
    <ns1:modelCreationInfo creationDate="2013-06-26 11:06:21.849" author="mom-xml-transformer-tool">
        <ns1:derivedModel urn="/oss_edt/ERBS_NODE_MODEL_D/ERBS_NODE_MODEL_D/0.0.1"/>
    </ns1:modelCreationInfo>
    <ns1:desc>UpgradePackageState</ns1:desc>
    <ns2:member value="0" ns1:name="NOT_INSTALLED" ns1:lifeCycle="NORMAL">
        <ns1:desc>The upgrade package is not installed.</ns1:desc>
    </ns2:member>
    <ns2:member value="1" ns1:name="INSTALL_COMPLETED" ns1:lifeCycle="NORMAL">
        <ns1:desc>The upgrade package has been completely installed.</ns1:desc>
    </ns2:member>
    <ns2:member value="2" ns1:name="UPGRADE_EXECUTING" ns1:lifeCycle="NORMAL">
        <ns1:desc>Upgrade (the action update, upgrade or rebootNodeUpgrade) is executing.</ns1:desc>
    </ns2:member>
    <ns2:member value="3" ns1:name="AWAITING_CONFIRMATION" ns1:lifeCycle="NORMAL">
        <ns1:desc>The execution of upgrade needs confirmation before it proceeds.</ns1:desc>
    </ns2:member>
    <ns2:member value="4" ns1:name="ONLY_DELETEABLE" ns1:lifeCycle="NORMAL">
        <ns1:desc>In this state the only allowed operations are delete and action updateFTPServerData. If any other type of action is invoked exception ActionNotAllowedException is thrown.</ns1:desc>
    </ns2:member>
    <ns2:member value="5" ns1:name="INSTALL_EXECUTING" ns1:lifeCycle="NORMAL">
        <ns1:desc>Installation of the UpgradePackage or PiuType MO is executing.</ns1:desc>
    </ns2:member>
    <ns2:member value="6" ns1:name="INSTALL_NOT_COMPLETED" ns1:lifeCycle="NORMAL">
        <ns1:desc>The upgrade package is not completely installed, that is, only parts of it has been installed.</ns1:desc>
    </ns2:member>
    <ns2:member value="7" ns1:name="UPGRADE_COMPLETED" ns1:lifeCycle="NORMAL">
        <ns1:desc>The upgrade (the action update, upgrade or rebootNodeUpgrade) has been successfully executed. 
				
				If this was the current state when an action was invoked it will also be the end state.</ns1:desc>
    </ns2:member>
    <ns2:member value="8" ns1:name="RESTORE_SU_EXECUTING" ns1:lifeCycle="NORMAL">
        <ns1:desc>Restore of the SU function is executing.</ns1:desc>
    </ns2:member>
    <ns2:member value="9" ns1:name="VERIFICATION_EXECUTING" ns1:lifeCycle="NORMAL">
        <ns1:desc>Verification of the upgrade package is executing.</ns1:desc>
    </ns2:member>
</ns2:EnumDataTypeDefinition>
